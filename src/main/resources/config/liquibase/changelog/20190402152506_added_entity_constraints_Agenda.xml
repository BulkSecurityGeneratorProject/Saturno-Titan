<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Agenda.
    -->
    <changeSet id="20190402152506-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="agenda_id"
                                 baseTableName="agenda_horario"
                                 constraintName="fk_agenda_horario_agenda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agenda"/>
        <addForeignKeyConstraint baseColumnNames="horarios_id"
                                 baseTableName="agenda_horario"
                                 constraintName="fk_agenda_horario_horarios_id"
                                 referencedColumnNames="id"
                                 referencedTableName="horario"/>
        
        <addForeignKeyConstraint baseColumnNames="agenda_id"
                                 baseTableName="agenda_horario_especial"
                                 constraintName="fk_agenda_horario_especial_agenda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agenda"/>
        <addForeignKeyConstraint baseColumnNames="horario_especials_id"
                                 baseTableName="agenda_horario_especial"
                                 constraintName="fk_agenda_horario_especial_horario_especials_id"
                                 referencedColumnNames="id"
                                 referencedTableName="horario_especial"/>
        
        <addForeignKeyConstraint baseColumnNames="agenda_id"
                                 baseTableName="agenda_dia_no_laborable"
                                 constraintName="fk_agenda_dia_no_laborable_agenda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agenda"/>
        <addForeignKeyConstraint baseColumnNames="dia_no_laborables_id"
                                 baseTableName="agenda_dia_no_laborable"
                                 constraintName="fk_agenda_dia_no_laborable_dia_no_laborables_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dia_no_laborable"/>
        
    </changeSet>
</databaseChangeLog>
